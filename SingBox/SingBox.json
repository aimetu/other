{
    "log": {
        "disabled": false,
        "level": "info",
        "output": "box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "local-dns",
                "address_resolver": "223.5.5.5",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "address": [
                    "https://doh.pub/dns-query",
                    "https://dns.alidns.com/dns-query",
                    "quic://dns.alidns.com",
                ],
                "detour": "DIRECT"
            },
            {
                "tag": "proxy-dns",
                "address_resolver": "119.29.29.29",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "address": [
                    "https://dns.google/dns-query",
                    "https://dns.opendns.com/dns-query"
                    "https://cloudflare-dns.com/dns-query"
                ],
                "detour": "🔰 Pick"
            },
            {
                "tag": "block-dns",
                "address": "rcode://success"
            },
            {
                "tag": "fakeip-dns",
                "address": "fakeip"
            }            
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "local-dns",
                "disable_cache": true
            },
            {
                "domain_suffix": [
                    "local",
                    "localhost"
                ],
                "server": "local-dns"
            },
            {
                "clash_mode": "DIRECT",
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "local-dns"
            },
            {
                "clash_mode": "🌐 Global",
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "proxy-dns"
            },
            {
                "rule_set": [
                    "ads"
                ],
                "server": "block-dns",
                "disable_cache": true,
                "rewrite_ttl": 0
            },
            {
                "rule_set": [
                    "proxy"
                ],
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "rewrite_ttl": 1,
                "server": "fakeip-dns"
            },
            {
                "rule_set": [
                    "cn"
                ],
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "local-dns"
            },
            {
                "fallback_rules": [
                    {
                        "rule_set": [
                            "cnip"
                        ],
                        "server": "local-dns"
                    },
                    {
                        "match_all": true,
                        "server": "fakeip-dns",
                        "rewrite_ttl": 1
                    }
                ],
                "server": "proxy-dns"
            }
        ],
        "final": "proxy-dns",
        "strategy": "prefer_ipv4",
        "independent_cache": true,
        "lazy_cache": true,
        "reverse_mapping": true,
        "mapping_override": true,
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18",
            "exclude_rule": {
                "rule_set": [
                    "fakeip-filter",
                    "private"
                ]
            }
        }
    },
