# 💡 通用设置
[General]
# 跳过代理或者ip段，不经过代理
skip-proxy = 

# 指定 ip 段范围绕过 tun 旁路由
bypass-tun = 

# 配置的域名不会使用 fakeip 进行映射，返回真实的 ip
real-ip = 

# 设置 dns 服务器
dns-server = 

# 💡 指定域名解析
[host]

